SELECT DISTINCT Title
FROM SalesLT.Customer;

SELECT count(*) 
FROM SalesLT.Product;

SELECT DISTINCT sp.Name 
FROM SalesLT.Product sp;

' Product Color'
SELECT DISTINCT sp.Color 
FROM SalesLT.Product sp;

select top 10 * from SalesLT.SalesOrderHeader
select top 10 * from SalesLT.Customer
select top 10 * from SalesLT.CustomerAddress

select count(*) from SalesLT.SalesOrderHeader so
inner join SalesLT.Customer sc
on so.CustomerID = sc.CustomerID


select  count(*) from SalesLT.Customer  
where customerid  not in (
    select   customerid from SalesLT.SalesOrderHeader 
      )


select  count(*) from SalesLT.SalesOrderHeader  
where customerid not  in (
    select   customerid from SalesLT.Customer 
      )

 
 

 
